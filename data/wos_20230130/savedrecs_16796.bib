@article{ WOS:000522809300002,
Author = {Rendon-Franco, Emilio and Lopez-Diaz, Osvaldo and Rodriguez-Espinosa,
   Oscar and Rojas-Serrania, Nora and Rodriguez-Cabo-Mercado, Roberto and
   Moreno-Altamirano, Maria M. B. and Munoz-Garcia, I, Claudia and
   Villanueva-Garcia, Claudia and Aguilar-Setien, Alvaro},
Title = {Comparative leucocyte populations between two sympatric carnivores
   (Nasua narica and Procyon lotor)},
Journal = {CONSERVATION PHYSIOLOGY},
Year = {2019},
Volume = {7},
Month = {OCT 11},
Abstract = {Coatis (Nasua narica) and raccoons (Procyon lotor) potentially play an
   important role in zoonotic diseases because they may carry pathogens and
   can transmit them to humans. To date, our understanding of the immune
   function of these two carnivores is deficient. The aim of this study was
   to compare the number of leucocyte subtypes and the phagocytic capacity
   between the coati and the raccoon. Blood samples were collected, and
   leucocyte subtypes were characterized and counted by flow cytometry and
   microscopy, respectively. Phagocytosis was analysed by kinetic assay.
   Differences in leucocytes between these two species were found; the
   total count of neutrophils was higher in raccoons than in coatis, but
   lymphocytes and eosinophils were higher in coatis than in raccoons.
   Antigen reduction was more rapid for the coatis. However, raccoons had a
   higher efficient endocytic process than coatis. This study provides the
   basis for understanding the procyonid immune system, which informs
   conservation, particularly since some procyonids are imperilled.},
