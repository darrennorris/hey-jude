@article{ WOS:000339720200020,
Author = {Sepulveda, Maximiliano A. and Singer, Randall S. and Silva-Rodriguez,
   Eduardo A. and Eguren, Antonieta and Stowhas, Paulina and Pelican,
   Katherine},
Title = {Invasive American Mink: Linking Pathogen Risk Between Domestic and
   Endangered Carnivores},
Journal = {ECOHEALTH},
