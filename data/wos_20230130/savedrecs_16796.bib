   Malo, Juan E.; Acebes, Pablo; Traba, Juan, Univ Autonoma Madrid, Dept Ecol, Terr Ecol Group TEG, E-28049 Madrid, Spain.},
DOI = {10.1007/s10531-011-0136-7},
ISSN = {0960-3115},
EISSN = {1572-9710},
